{
	"supports": {
		"jetStyle": {
			"--choice-color": [
				".jet-form-builder-choice--item",
				"color",
				"text"
			],
			"--choice-checked-color": [
				".jet-form-builder-choice--item.is-checked",
				"color",
				"text"
			]
		}
	},
	"value": {},
	"label": "choices",
	"name": "choices",
	"style": {
		"border": {
			"radius": {
				"topLeft": "2em",
				"topRight": "4em",
				"bottomLeft": "4em",
				"bottomRight": "2em"
			},
			"top": {
				"color": "var:preset|color|secondary",
				"width": "8px"
			},
			"right": {
				"color": "var:preset|color|vivid-green-cyan",
				"width": "4px"
			},
			"bottom": {
				"color": "var:preset|color|secondary",
				"width": "8px"
			},
			"left": {
				"color": "var:preset|color|vivid-green-cyan",
				"width": "4px"
			}
		}
	},
	"layout": {
		"type": "flex",
		"justifyContent": "space-between"
	}
	"attributes": {
		"style": {
			"color": {
				"background": "#somecolor",
				"text": "#somecolor"
			},
			"border": {
				"width": "1px",
				"style": "solid",
				"color": "#somecolor"
			},
			".jet-form-builder-choice--item": {
				"color": {
					"background": "#default-color",
					"text": "#default-color"
				}
			},
			".jet-form-builder-choice--item.is-checked": {
				"color": {
					"background": "#checked-color",
					"text": "#checked-color"
				}
			},
			".jet-form-builder-choice--item:hover": {
				"color": {
					"background": "#checked-color",
					"text": "#checked-color"
				}
			}
		}
	}
}