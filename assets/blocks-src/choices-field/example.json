{
	"supports": {
		"jetStyle": {
			"--choice-color": [
				".jet-form-builder-choice--item",
				"color",
				"text"
			],
			"--choice-checked-color": [
				".jet-form-builder-choice--item.is-checked",
				"color",
				"text"
			]
		}
	},
	"attributes": {
		"style": {
			"color": {
				"background": "#somecolor",
				"text": "#somecolor"
			},
			"border": {
				"width": "1px",
				"style": "solid",
				"color": "#somecolor"
			},
			".jet-form-builder-choice--item": {
				"color": {
					"background": "#default-color",
					"text": "#default-color"
				}
			},
			".jet-form-builder-choice--item.is-checked": {
				"color": {
					"background": "#checked-color",
					"text": "#checked-color"
				}
			},
			".jet-form-builder-choice--item:hover": {
				"color": {
					"background": "#checked-color",
					"text": "#checked-color"
				}
			}
		}
	}
}