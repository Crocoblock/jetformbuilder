{
	"supports": {
		"jetStyle": {
			"--choice-color": {
				"selector": ".jet-form-builder-choice--item",
				"attrs":[
					"color",
					"text"
				]
			},
			"--choice-checked-color": {
				"selector": ".jet-form-builder-choice--item.is-checked",
				"attrs":[
					"color",
					"text"
				]
			}
		}
	},
	"attributes": {
		"style": {
			"color": {
				"background": "#somecolor",
				"text": "#somecolor"
			},
			"border": {
				"width": "1px",
				"style": "solid",
				"color": "#somecolor"
			},
			".jet-form-builder-choice--item": {
				"color": {
					"background": "#default-color",
					"text": "#default-color"
				},
				"&.is-checked": {
					"color": {
						"background": "#checked-color",
						"text": "#checked-color"
					},
					":hover": {
						"color": {
							"background": "#checked-with-hover-color",
							"text": "#checked-with-hover-color"
						}
					}
				},
				":hover": {
					"color": {
						"background": "#checked-color",
						"text": "#checked-color"
					}
				}
			}
		}
	}
}