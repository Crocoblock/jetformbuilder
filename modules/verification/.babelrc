{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": [
						"> 1%",
						"last 2 versions",
						"not ie <= 9"
					]
				}
			}
		]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		[
			"@babel/plugin-transform-react-jsx",
			{
				"pragma": "wp.element.createElement"
			}
		]
	]
}